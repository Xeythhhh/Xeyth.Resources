{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "upgrade",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#111111",
          "background": "#FFD664",
          "properties": {
            "cache_duration": "168h"
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#689f63",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": true
          },
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 "
        },
        {
          "type": "go",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#00acd7",
          "foreground": "#111111",
          "properties": {
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 "
        },
        {
          "type": "julia",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#4063D8",
          "foreground": "#111111",
          "properties": {
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue624 "
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#FFDE57",
          "foreground": "#111111",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 "
        },
        {
          "type": "ruby",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#AE1401",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 "
        },
        {
          "type": "azfunc",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7"
        }
      ]
    },

    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [

        {
          "type": "os",
          "background": "#929292",
          "foreground": "#000000",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
        },
        {
          "background": "#ffff66",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0ad ",
          "type": "root"
        },
        {
          "background": "#c4a000",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .FormattedMs }} \uf252 ",
          "type": "executiontime"
        },
        {
          "background": "#009e15",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "foreground": "#d3d7cf",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status"
        },
        
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background_templates": [
            "{{ if .Writable }}#3465a4{{ else }}#FE9900{{ end }}"
          ],
          "foreground_templates": [
            "{{ if .Writable }}#defa96{{ else }}#F50003{{ end }}"
          ],
          "properties": {
            "home_icon": "~",
            "style": "powerlevel",
            "max_width": 70
          },
          "template": " \uf07c {{ if .Writable }}{{ else }}üîí {{ end }}{{ .Path }} "
        }
      ]
    },

    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "show_if": "{{ .Project.Name }}",
      "segments": [
        {
          "type": "project",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "template": " {{ .Type }}"
        },
        {
          "type": "project",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#193549",
          "background": "#ff3b89",
          "template": " {{ .Target }}"
        },
        {
          "type": "project",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#193549",
          "background": "#0bff03",
          "template": " {{ .Version }}"
        },
        {
          "type": "project",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#00ea94",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Name }}{{ end }} "
        }
      ]
    },

    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#b8c5ff",
          "background": "#bc2248",
          "template": " {{.UpstreamIcon }} {{ .RepoName }}",
          "properties": {
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "az",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#000000",
          "background": "#9ec3f0",
          "template": " ÓØò {{ .TenantDisplayName}} | {{ .Name }} | {{ .EnvironmentName }}"
        },
        {
          "type": "terraform",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#000000",
          "background": "#ebcc34",
          "template": "{{ .WorkspaceName }}{{ if .Version }} {{ .Version }}{{ end }}",
          "properties": {
            "fetch_version": true
          }
        },

        {
          "type": "git",
          "background": "#59C9A5",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#59C9A5{{ end }}"
          ],
          "foreground": "#262B44",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": " \uf126 ",
            "branch_identical_icon": "‚úÖ",
            "branch_ahead_icon": "üî•",
            "branch_behind_icon": "‚ö†",
            "branch_gone_icon": "üíÄ",
            "mapped_branches": {
              "release/*": "üí≤ ",
              "feature/*": "üöÄ ",
              "bugfix/*": "üêõ ",
              "hotfix/*": "üí• "

            },

            "fetch_stash_count": true,
            "fetch_status": true
          },
          "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#f89d0c",
          "template": "{{ if .LatestTag }}üè∑Ô∏è {{ .LatestTag }}{{ end }}"
        }
      ]
    }

  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 3,
  "upgrade": {
    "source": "cdn",
    "auto": false,
    "notice": true,
    "interval": "168h"
  }
}
